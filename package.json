{
  "name": "git-fit",
  "version": "1.0.0",
  "engines": {
    "node": "18.x"
  },
  "description": "![GitFitLogo](client/public/logoShape.png)",
  "main": "server/server.js",
  "scripts": {
    "start": "node server --ignore client",
    "develop": "concurrently \"cd server && npm run start\" \"cd client && npm run dev\"",
